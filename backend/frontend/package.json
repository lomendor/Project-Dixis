{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "dev:e2e": "next dev -p 3001 || vite --port 3001",
    "dev:api": "php -S 127.0.0.1:8001 -t ../public",
    "api:e2e": "php ../artisan serve --host 127.0.0.1 --port 8001",
    "e2e:ci:setup": "npm run build && npm run start -- -p 3001 &",
    "e2e:ci": "playwright test --reporter=html,list"
  },
  "dependencies": {
    "next": "15.5.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}